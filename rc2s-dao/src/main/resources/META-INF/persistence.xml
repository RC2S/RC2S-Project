<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence         http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="rc2s" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/rc2s-ds</jta-data-source>
	
    <class>com.rc2s.common.vo.User</class>
    <class>com.rc2s.common.vo.Group</class>
    <class>com.rc2s.common.vo.Synchronization</class>
    <class>com.rc2s.common.vo.Cube</class>
    <class>com.rc2s.common.vo.Size</class>
    <class>com.rc2s.common.vo.Plugin</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
    </properties>
  </persistence-unit>
</persistence>
